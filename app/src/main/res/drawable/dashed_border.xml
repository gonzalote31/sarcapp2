<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <solid android:color="@android:color/transparent" />
            <stroke
                android:width="6dp"
                android:color="#0D47A1"
                android:dashGap="2dp"
                android:dashWidth="3dp" />
